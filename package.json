{
  "name": "boilerplate-react-parcel",
  "version": "1.0.0",
  "description": "Boilerplate for React",
  "main": "index.js",
  "scripts": {
    "dev": "npm run tailwind:css && parcel src/index.html",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\" ",
    "tailwind:css": "tailwind build src/styles/tailwind.src.css -c tailwind.js -o src/styles/tailwind.css"
  },
  "author": "Ralph Cacho",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.16.4",
    "stylelint-config-recommended": "^2.1.0",
    "tailwindcss": "^0.7.4"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  }
}
